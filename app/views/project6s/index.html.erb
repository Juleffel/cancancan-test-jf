<p id="notice"><%= notice %></p>

<h1>Project6s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project6s.each do |project6| %>
      <tr>
        <td><%= project6.name %></td>
        <td><%= project6.company %></td>
        <td><%= link_to 'Show', company_project6_path(@company, project6) %></td>
        <td><%= link_to 'Custom member', custom_member_company_project6_path(@company, project6) %></td>
        <td><%= link_to 'Edit', edit_company_project6_path(@company, project6) %></td>
        <td><%= link_to 'Destroy', company_project6_path(@company, project6), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

